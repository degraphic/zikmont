    <hr>

    <a class="close-reveal-modal">&#215;</a>
</div>

<div class="container">          
    {% render "zikmontFrontEndBundle:Mensajes:mostrar" %}    
    {% block content %}

    {% endblock %}
</div>
{% endblock %}